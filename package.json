{
  "name": "svelte-seed",
  "version": "1.0.0",
  "description": "A seed for starting svelte projects",
  "main": "index.js",
  "author": "Antony Jones",
  "license": "MIT",
  "scripts": {
    "clean": "rm ./dist/*",
    "build": "NODE_ENV=production rollup -c src/rollup.config.js",
    "watch": "NODE_ENV=development rollup -wc src/rollup.config.js",
    "zip": "npm run build && bestzip ./dist/package.zip ./dist/*",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register './!(node_modules)/**/**.+(spec).js'"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bestzip": "^1.1.4",
    "code": "^4.1.0",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-svelte": "^2.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-url": "^1.2.0",
    "rollup-watch": "^4.3.1",
    "svelte": "^1.26.0"
  },
  "dependencies": {}
}
